{
  "Serilog": {
    "Enrich": [
      "FromLogContext",
      "WithExceptionDetails",
      "WithEnvironmentUserName",
      "WithMachineName",
      "WithProcessId",
      "WithProcessName",
      "WithSpan",
      "WithThreadId"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "CacheProfiles": {
    "StaticFiles": {
      "Duration": 31536000,
      "Location": "Any"
    }
  },
  "Compression": {
    "MimeTypes": [
      "application/vnd.restful+json",
      "application/problem+json",
      "image/x-icon"
    ]
  },
  "ForwardedHeaders": {
    "AllowedHosts": [
      "*"
    ],
    "ForwardedHeaders": "XForwardedFor,XForwardedHost,XForwardedProto"
  },
  "Redis": {
    "ConnectionString": "redis:6379"
  },
  "Host": {
    "ShutdownTimeout": "00:03:00"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:80"
      }
    },
    "Limits": {
      "MaxRequestBodySize": 10240,
      "MaxRequestHeaderCount": 40
    }
  },
  "Authentication": {
    "Google": {
      "ClientId": "XXX",
      "ClientSecret": "XXX",
      "Authority": "https://accounts.google.com/"
    }
  },
  "ConnectionStrings": {
    "ConfigurationStoreConnectionString": "Server=DB_HOST_NAME;Port=25060;Database=ConfigurationStore;Userid=XXX;Password=XXX;SslMode=Required;",
    "OperationalStoreConnectionString": "Server=DB_HOST_NAME;Port=25060;Database=OperationalStore;Userid=XXX;Password=XXX;SslMode=Required;",
    "ApplicationStoreConnectionString": "Server=DB_HOST_NAME;Port=25060;Database=OperationalStore;Userid=XXX;Password=XXX;SslMode=Required;",
  }
}
